@model IEnumerable<WebsiteHouseBanDoNoiThat.Model.DoiTuong>

@{
    ViewBag.Title = "Index";
}

<h2>Danh sách đối tượng</h2>

<p>
    @Html.ActionLink("Thêm mới", "Create")
</p>
<table class="table table-bordered table-hover">
    <tr>
        <th>
            Mã Đối Tượng
        </th>
        <th>
            Tên đối tượng
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaDoiTuong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenDoiTuong)
        </td>
        <td>
            @Html.ActionLink("Chỉnh sửa", "Edit", new { id = item.MaDoiTuong, }) |
            @Html.ActionLink("Chi tiết", "Details", new { id = item.MaDoiTuong }) |
            @Html.ActionLink("Xóa", "Delete", new { id = item.MaDoiTuong })
        </td>
    </tr>
    }

</table>
